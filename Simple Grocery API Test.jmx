          <JSONPostProcessor guiclass="JSONPostProcessorGui" testclass="JSONPostProcessor" testname="JSON Extractor" enabled="true">
            <stringProp name="JSONPostProcessor.referenceNames">coffeeProductIds</stringProp>
            <stringProp name="JSONPostProcessor.jsonPathExprs">$[?(@.inStock == true &amp;&amp; @.category == &quot;coffee&quot;)].id </stringProp>
            <stringProp name="JSONPostProcessor.match_numbers"></stringProp>
            <stringProp name="Sample.scope">all</stringProp>
            <stringProp name="JSONPostProcessor.defaultValues">Product not available</stringProp>
