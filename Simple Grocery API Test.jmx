          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="JSR223 PostProcessor" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import java.util.Random

// Extract product IDs
def productIds = vars.get(&quot;coffeeProductIds&quot;).split(&quot;,&quot;)
log.info(&quot;Extracted Coffee Product IDs: &quot; + vars.get(&quot;coffeeProductIds&quot;));


// Check if there are products available
if (productIds.length &gt; 0) {
    // Pick a random product ID
    def randomIndex = new Random().nextInt(productIds.length)
    def randomProductId = productIds[randomIndex].trim()

    // Store the selected product ID in a global property
    props.put(&quot;randomCoffeeProductId&quot;, randomProductId)
    log.info(&quot;Random Coffee Product ID: &quot; + randomProductId)
} else {
    log.error(&quot;No coffee products found that are in stock.&quot;)
}
</stringProp>
          </JSR223PostProcessor>
